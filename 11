 #include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin >> n;
    while(n--) {
string s;
cin >> s;
sort(s.begin(), s.end());
    int i;
for(i = 1; i < s.size(); i++) {
if(s[i] == s[i-1] || s[i] != s[i-1] + 1) {
cout << "No\n";
break;
}
}
    if(i == s.size())
            cout << "Yes\n";
    }
    return 0;
}
